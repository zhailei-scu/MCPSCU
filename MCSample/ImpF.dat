	 	!&INSERTTIMEPOINT The batch insert time point = 2.0D-7 , 2.0D-6 , 2.0D-5 , 2.0D-4 , 2.0D-3

	 	&INSERTCOUNTMODEL The count model for each batch = 1           	! 0 stands for by counts of configuration number, 1 stands for by counts of cluster number
	 	&INSERTCOUNT  The count in each batch = 10	       	       	! the count in each batch
	 &ENDSUBCTL
        
         &EXTFSUBCTL
	    &DISTFILETYPE The distribution file type = "DISTPANDA" , Implant ion = "He"                       ! or "BOXMF18", "BOXSPMF18", "DISTSRIM","DISTPANDA"
													      ! "CFGOKMC18", "CFGSRIM" , "CFGMARLOWER" , "CFGMD"
								                         		      ! if the DISTPANDA or DISTSRIM is formatted, you should special the ion type
													      ! if the DISTSRIM speicaled, you should also give layers number you want

	    !&DISTFILETYPE The distribution file type = "DISTSRIM" , Implant ion = "He" , Layer number = 100 

	    !&DISTFILETYPE The distribution file type = "BOXMF18"

	    !&DISTFILETYPE The distribution file type = "BOXSPMF18"	
	
            &FOLDER The candiacate batches configurations folder = "PANDA/"
	    &SELECTSEQUENCE select sequence is by = 0			!  0 stands by file name alphabetical order , 1 stands by random choosen configure file
         &ENDSUBCTL
  &ENDSUBCTL

