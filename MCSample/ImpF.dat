&IMPFINPUTF

  &GROUPSUBCTL   ! #1
	 &IMPCOMMONSUBCTL
         	&SOURCETYPE The implantation cluster source type = 0 	        ! 0 by simple inner way, 1 by from external file, 2 by function

		&FEXPAND The expand size factor = 1

         	&FMEMOCCUP The memory occupied factor = 100
	 &ENDSUBCTL

	 &RATESUBCTL
	   	&TIMEINTERVAL The time interval between batches = 2.0D-7       	! if batches model is selected, you must special the time interval
	 	!&INSERTTIMEPOINT The batch insert time point = 2.0D-7 , 2.0D-6 , 2.0D-5 , 2.0D-4 , 2.0D-3

	 	&INSERTCOUNTMODEL The count model for each batch = 1           	! 0 stands for by counts of configuration number, 1 stands for by counts of cluster number
	 	&INSERTCOUNT  The count in each batch = 10	       	       	! the count in each batch
	 &ENDSUBCTL

         &SIZESUBCTL
            &NATOMDIST The atoms number in each cluster distribution as Gauss that central = 1 , distribution half width = 0.0 , left cut = 0.9 , right cut = 1.1
            &ELEMENTCOMPOSIT The includeed element = "He", "H" ,The includeed element = (1, 0)
         &ENDSUBCTL

         &DEPTHSUBCTL ! only used the first distribution in below
            &DEPTH_LAYER THE NUMBER OF DEPTH DISTRIBUTION LAYER = 2,THE THINCKNESS DISTRIBUTION = (0.01,0.99) , THE ENTRIES DISTRIBUTION = (1,0)
            !&DEPTH_SUBBOX THE SUBOX SHAPE IS THAT: X = 50, Y = 50, Z = 50
            !&DEPTH_GAUSS THE GAUSS DISTRIBUTION CENTRAL = 50, THE HALF WIDTH = 0
         &ENDSUBCTL
  &ENDSUBCTL

  &GROUPSUBCTL  ! #2

	 &IMPCOMMONSUBCTL
         	&SOURCETYPE The implantation cluster source type = 1 	        ! 0 by simple inner way, 1 by from external file, 2 by function

		&FEXPAND The expand size factor = 1

         	&FMEMOCCUP The memory occupied factor = 100
