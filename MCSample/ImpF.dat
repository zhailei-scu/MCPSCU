	 &ENDSUBCTL
        
         &EXTFSUBCTL
	    &DISTFILETYPE The distribution file type = "DISTPANDA" , Implant ion = "He"                       ! or "DISTOKMC18","BOXMF18", "BOXSPMF18", "DISTSRIM","DISTPANDA"
								                         		      ! if the DISTPANDA or DISTSRIM is formatted, you should special the ion type
													      ! if the DISTSRIM or DISTOKMC18 is speicaled, you should also give layers number you want

	    !&DISTFILETYPE The distribution file type = "DISTSRIM" , Implant ion = "He" , Layer number = 100 

	    !&DISTFILETYPE The distribution file type = "DISTOKMC18" , Layer number = 100

	    !&DISTFILETYPE The distribution file type = "BOXMF18"

	    !&DISTFILETYPE The distribution file type = "BOXSPMF18"	
	
